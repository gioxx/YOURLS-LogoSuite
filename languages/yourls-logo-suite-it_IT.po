msgstr "Impostazioni Logo"

#: plugin-file.php:103
msgid "Image URL"
msgstr "URL Immagine"

#: plugin-file.php:105
msgid "Example: a direct link to your logo image (PNG, JPG, SVG)."
msgstr "Esempio: un link diretto alla tua immagine logo (PNG, JPG, SVG)."

#: plugin-file.php:110
msgid "ALT Tag"
msgstr "Tag ALT"

#: plugin-file.php:112
msgid "Example: descriptive text for accessibility."
msgstr "Esempio: testo descrittivo per accessibilità."

#: plugin-file.php:117
msgid "Title Attribute"
msgstr "Attributo Titolo"

#: plugin-file.php:119
msgid "Example: tooltip text shown on hover."
msgstr "Esempio: testo del tooltip mostrato al passaggio del mouse."

#: plugin-file.php:125
msgid "Page Title Settings"
msgstr "Impostazioni Titolo Pagina"

#: plugin-file.php:128
msgid "Custom Title"
msgstr "Titolo Personalizzato"

#: plugin-file.php:130
msgid "Example: your custom page title."
msgstr "Esempio: il tuo titolo personalizzato della pagina."

